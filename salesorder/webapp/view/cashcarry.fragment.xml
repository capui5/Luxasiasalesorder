<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <Dialog
        id="cashcarrydialog"
        title="CREATE NEW PROFILE"
        showHeader="false"
    >
        <VBox
            xmlns="sap.m"
            class="sapUiContentPadding"

        >
            <HBox alignItems="Center">
                <Label
                    class="fragmentext1"
                    text="CASH &amp; CARRY"
                />

                <HBox width="100%" />
                 <Select items="" id="CASHCARRY" selectedKey="{Country}">
									<items>
										<core:Item text="CNCLOCAL" key="1"/>
                                        <core:Item text="CNCTOURIST" key="2"/>
									</items>
								</Select>

             <Button
                    id="helloDialogButton"
                    icon="sap-icon://decline"
                    press=".onCloseFrag"
                />
            </HBox>
        </VBox>
       
        
        <HBox class="fragtile">
            <GenericTile
                class="sapUiTinyMarginBegin sapUiTinyMarginTop tilesetting products "
                header="SCAN PRODUCTS"
                subheader="Subtitle"
                press="onScanBarcodecash"
                frameType="TwoByHalf"
            >
                <TileContent
                    unit="EUR"
                    footer="Current Quarter"
                >
                    <ImageContent src="sap-icon://bar-code" class="newprofile" />
                </TileContent>
            </GenericTile>

            <GenericTile
                class="sapUiTinyMarginBegin sapUiTinyMarginTop tilesetting"
                header="SEARCH FOR PRODUCTS"
                press="onSearchProduct"
                frameType="TwoByHalf"
            >
                <TileContent
                    unit="EUR"
                    footer="Current Quarter"
                >
                    <ImageContent src="sap-icon://search" class="newprofile"/>
                </TileContent>
            </GenericTile>
        </HBox>
    </Dialog>
</core:FragmentDefinition>
